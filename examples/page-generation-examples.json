  "api_usage": {
    "endpoint": "/api/generate-page",
    "method": "POST",
    "headers": {
      "Content-Type": "application/json"
    },
    "request_body_structure": {
      "pageName": "string (required) - The name of the page to generate",
      "content": "object (required) - The page content following PageData interface"
    },
    "response_structure": {
      "success": "boolean - Whether the page was generated successfully",
      "message": "string - Success/error message",
      "data": {
        "pageName": "string - The name of the generated page",
        "pageUrl": "string - The URL where the page can be accessed",
        "sectionsCount": "number - Number of sections in the page",
        "sections": "array - Array of section types used",
        "generatedFiles": "array - Array of generated file paths"
      }
    }
  },
  "curl_examples": {
    "simple_landing": "curl -X POST http://localhost:3000/api/generate-page -H 'Content-Type: application/json' -d @simple-landing.json",
    "business_page": "curl -X POST http://localhost:3000/api/generate-page -H 'Content-Type: application/json' -d @business-page.json",
    "minimal_page": "curl -X POST http://localhost:3000/api/generate-page -H 'Content-Type: application/json' -d @minimal-page.json"
  },
  "javascript_examples": {
    "fetch_api": "const response = await fetch('/api/generate-page', { method: 'POST', headers: { 'Content-Type': 'application/json' }, body: JSON.stringify({ pageName: 'my-page', content: pageData }) });",
    "axios": "const response = await axios.post('/api/generate-page', { pageName: 'my-page', content: pageData });"
  }
